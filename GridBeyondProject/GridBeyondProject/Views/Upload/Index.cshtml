@{
    ViewBag.Title = "Upload";
}

<h2>Upload a file</h2>

@using (Html.BeginForm("SaveFile",
                        "Upload",
                        FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
{
    <label for="file">Upload a csv file:</label>
    <input type="file" name="file" id="file" />
    <br />

    <input type="submit" value="Send" />
    <br>

    @ViewBag.Message
}